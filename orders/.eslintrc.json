{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["@typescript-eslint/eslint-plugin"],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "eslint-config-prettier"
    ],
    "rules": {
      "strict": ["error", "never"]
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    }
}
